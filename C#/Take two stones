using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
 

    class Program
    {
        static void Main(string[] args)
        {
            int i;
            
            i = int.Parse(Console.ReadLine());
            if (i % 2 == 0)
            {
                Console.Write("Bob");
                Console.Read();
            }
            else
            {
                Console.Write("Alice");
                Console.Read();
            }
        }
    }
